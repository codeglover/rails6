

<%= turbo_frame_tag dom_id(@post) do %>

    <p id="notice"><%= notice %></p>
    <p>
      <strong>Title:</strong>
      <%= @post.title %>
    </p>

    <p>
      <strong>Publish date:</strong>
      <%= @post.publish_date %>
    </p>

    <p>
    <%= link_to 'Edit', edit_post_path(@post) %> |
    <%= link_to 'Back', posts_path %>
    </p>
<% end %>




